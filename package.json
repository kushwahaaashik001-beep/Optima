    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "@types/framer-motion": "^10.16.16",
    "@types/react-hot-toast": "^2.2.0"
  },
  "engines": {
    "node": ">=18.17.0",
